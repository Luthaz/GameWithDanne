<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import java.net.URL?>
<BorderPane fx:controller="Controller"
            xmlns:fx="http://javafx.com/fxml">
 
    <stylesheets>
        <URL value="@css.css"/>
    </stylesheets>
 
    <center>
   	 	<Button text="Start" onAction="#handlePressMe" fx:id="startButton"/>
        <Button text="Options" onAction="#handlePressMe" fx:id="optionsButton"/>
        <Button text="Exit" onAction="#handlePressMe" fx:id="exitButton"/>
    </center>

</BorderPane>